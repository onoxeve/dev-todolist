<div class="childtask">
  <div class="card">
  <div class="card-block">
    <h4 class="card-title">
      <%= link_to childtask.name, parenttask_path(childtask.parenttask_id) %>
    </h4>
    <div class="card-text">
      期限： <%= l(childtask.due_date, format: :long) %>
    </div>
    <div class="card-text">
      作成日： <%= l(childtask.created_at, format: :long)  %>
    </div>
    <div class="card-text">
      リスト： <%= childtask.parenttask.name %>
    </div>
  </div>
  </div>
</div>
